    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.9.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "ts-loader": "^9.2.6"
  }
}
