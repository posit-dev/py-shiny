    "clean": "rimraf dist",
    "test": "npm run test:default",
    "test:default": "echo \"No test specified\""
  },
  "dependencies": {
    "@jupyter-widgets/html-manager": "^0.20.0",
    "@jupyter-widgets/base": "^0.20.0",
